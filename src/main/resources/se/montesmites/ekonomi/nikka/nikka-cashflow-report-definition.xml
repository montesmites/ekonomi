<?xml version="1.0" encoding="UTF-8"?>
<defintion>
    <section id="inkomster">
        <description>Inkomster</description>
        <account-group>
            <description>Löner och arvoden</description>
            <regex>(30|36)\d\d</regex>
        </account-group>
        <account-group>
            <description>Nettoomsättning övrigt</description>
            <regex>3([1-5]|[7-9])\d\d</regex>
        </account-group>
    </section>
    <section id="boende">
        <description>Boende</description>
        <account-group>
            <description>Månadsavgift m.m.</description>
            <regex>501\d</regex>
        </account-group>
        <account-group>
            <description>Amortering Fredsgatan 13</description>
            <regex>2353</regex>
        </account-group>
        <account-group>
            <description>Bolån ränta</description>
            <regex>84[01]\d</regex>
        </account-group>
        <account-group>
            <description>Hemförsäkring</description>
            <regex>5081</regex>
        </account-group>
        <account-group>
            <description>El (förbrukning och nät)</description>
            <regex>502\d</regex>
        </account-group>
        <account-group>
            <description>Mobil, tv, bredband</description>
            <regex>51\d\d</regex>
        </account-group>
    </section>
    <section id="förnödenheter">
        <description>Förnödenheter</description>
        <account-group>
            <description>Dagligvaror</description>
            <regex>40\d\d</regex>
        </account-group>
        <account-group>
            <description>Kläder och skor</description>
            <regex>41\d\d</regex>
        </account-group>
        <account-group>
            <description>Kropp och själ</description>
            <regex>42\d\d</regex>
        </account-group>
        <account-group>
            <description>Personförsäkringar</description>
            <regex>43\d\d</regex>
        </account-group>
        <account-group>
            <description>A-kassa, fack, bank, skatt</description>
            <regex>4[4-7]\d\d</regex>
        </account-group>
        <account-group>
            <description>Transporter</description>
            <regex>56\d\d</regex>
        </account-group>
    </section>
    <section id="övrigt">
        <description>Övrigt</description>
        <account-group>
            <description>Boende diverse</description>
            <regex>5060|5[458]\d\d</regex>
        </account-group>
        <account-group>
            <description>Övrigt</description>
            <regex>(4[89]|[67]\d)\d\d</regex>
        </account-group>
    </section>
    <section id="jämförelsestörande poster">
        <description>Jämförelsestörande poster</description>
        <account-group>
            <description>Kortfristigt netto</description>
            <regex>(1[5-8]|2[4-9])\d\d</regex>
        </account-group>
        <account-group>
            <description>Långsiktigt netto</description>
            <regex>(10\d|13[456])\d</regex>
        </account-group>
        <account-group>
            <description>Finansiellt netto</description>
            <regex>8(3\d|4[2-9])\d</regex>
        </account-group>
        <account-group>
            <description>Investering boende</description>
            <regex>11\d\d</regex>
        </account-group>
        <account-group>
            <description>Extraordinärt netto</description>
            <regex>87\d\d</regex>
        </account-group>
    </section>
    <section id="förändring likvida medel">
        <description>Förändring likvida medel</description>
        <account-group-ref id="likvida medel"/>
    </section>
    <account-group id="likvida medel">
        <description>Likvida medel</description>
        <regex>1493|19\d\d</regex>
    </account-group>
    <section id="före jämförelsestörande poster">
        <description>Före jämförelsestörande poster</description>
        <section-ref id="inkomster"/>
        <section-ref id="boende"/>
        <section-ref id="förnödenheter"/>
        <section-ref id="övrigt"/>
    </section>
    <section id="kontrollsumma">
        <description>Kontrollsumma</description>
        <section-ref id="inkomster"/>
        <section-ref id="boende"/>
        <section-ref id="förnödenheter"/>
        <section-ref id="övrigt"/>
        <section-ref id="jämförelsestörande poster"/>
        <section-ref id="förändring likvida medel"/>
    </section>
    <section id="ackumulerade likvida medel">
        <description>Ackumulerade likvida medel</description>
        <account-group-ref id="likvida medel"/>
    </section>
    <report>
        <section-ref id="inkomster"/>
        <section-ref id="boende"/>
        <section-ref id="förnödenheter"/>
        <section-ref id="övrigt"/>
        <section-ref id="före jämförelsestörande poster">
            <compact-section/>
            <totalling-section/>
        </section-ref>
        <section-ref id="jämförelsestörande poster"/>
        <section-ref id="förändring likvida medel">
            <compact-section/>
            <negated-section/>
        </section-ref>
        <section-ref id="kontrollsumma">
            <compact-section/>
            <totalling-section/>
        </section-ref>
        <section-ref id="ackumulerade likvida medel">
            <compact-section/>
            <accumulating-section/>
        </section-ref>
    </report>
</defintion>
